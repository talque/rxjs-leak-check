{
  "name": "rxjs-leak-check",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "build:lib": "ng build rxjs-leak-check-lib",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:lib": "jest projects/rxjs-leak-check-lib",
    "lint": "ng lint",
    "lint:lib": "ng lint --project rxjs-leak-check-lib",
    "publish:npm": "ng build rxjs-leak-check-lib --configuration production && cd dist/rxjs-leak-check-lib && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^18.2.10",
    "@angular/cdk": "^18.2.11",
    "@angular/common": "^18.2.10",
    "@angular/compiler": "^18.2.10",
    "@angular/core": "^18.2.10",
    "@angular/forms": "^18.2.10",
    "@angular/material": "^18.2.11",
    "@angular/platform-browser": "^18.2.10",
    "@angular/platform-browser-dynamic": "^18.2.10",
    "@angular/router": "^18.2.10",
    "@types/markdown-it": "^13.0.0",
    "error-stack-parser": "^2.1.4",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.1.0",
    "observable-profiler": "^0.1.1",
    "rxjs": "^7.5.0",
    "stacktrace-gps": "^3.1.2",
    "stacktrace-js": "^2.0.2",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-eslint/builder": "18.4.0",
    "@angular-eslint/eslint-plugin": "18.4.0",
    "@angular-eslint/eslint-plugin-template": "18.4.0",
    "@angular-eslint/schematics": "18.4.0",
    "@angular-eslint/template-parser": "18.4.0",
    "@angular/cli": "~18.2.11",
    "@angular/compiler-cli": "^18.2.10",
    "@types/jasmine": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "jasmine-core": "^5.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.0",
    "karma-coverage": "~2.2.0",
    "ng-packagr": "^18.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  }
