{
  "name": "rxjs-leak-check",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "build:lib": "ng build rxjs-leak-check-lib",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:lib": "jest projects/rxjs-leak-check-lib",
    "lint": "ng lint",
    "lint:lib": "ng lint --project rxjs-leak-check-lib",
    "publish:npm": "ng build rxjs-leak-check-lib --configuration production && cd dist/rxjs-leak-check-lib && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^17.3.5",
    "@angular/cdk": "^17.3.5",
    "@angular/common": "^17.3.5",
    "@angular/compiler": "^17.3.5",
    "@angular/core": "^17.3.5",
    "@angular/forms": "^17.3.5",
    "@angular/material": "^17.3.5",
    "@angular/platform-browser": "^17.3.5",
    "@angular/platform-browser-dynamic": "^17.3.5",
    "@angular/router": "^17.3.5",
    "@types/markdown-it": "^13.0.0",
    "error-stack-parser": "^2.1.4",
    "markdown-it": "^14.0.0",
    "markdown-it-highlightjs": "^4.0.1",
    "observable-profiler": "^0.1.1",
    "rxjs": "^7.5.0",
    "stacktrace-js": "^2.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.5",
    "@angular/compiler-cli": "^17.3.5",
    "@types/jasmine": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.57.0",
    "jasmine-core": "^5.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "karma-coverage": "~2.2.0",
    "ng-packagr": "^17.2.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
